meta:
  package: layer-0.MetaN
  name: type
  description: type relation and metaN type (all meta^n-types collapsed into one node)
  version: 1.0.0
  public: true
  meta-level: N
references:
nodes:
  metaN-typeNode: node that represents meta-meta-type, meta-meta-meta-type and so forth
  typeRelation: node representing the type relation, linking one meta-level to the next
relations:
  typeRelation -typeRelation-> metaN-TypeNode
  metaN-TypeNode -typeRelation-> metaN-TypeNode
